{
    "name": "lenmic/common-type-system",
    "description": "A PHP library for common data types.",
    "minimum-stability": "stable",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "collection",
        "tree",
        "map",
        "queue",
        "set"
    ],
    "authors": [
        {
            "name": "marsov",
            "email": "marsov@lenmic.de"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/dependency-injection": "^7.1",
        "symfony/config": "^7.1",
        "symfony/yaml": "^7.1",
        "illuminate/collections": "^11.32"
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "codeception/module-phpbrowser": "*",
        "codeception/module-asserts": "*"
    },
    "autoload": {
        "psr-4": {
            "LenMic\\CommonTypeSystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LenMic\\CommonTypeSystem\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "codecept build && codecept run"
    }
}
